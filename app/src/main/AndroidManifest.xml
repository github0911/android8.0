<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android80">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.movie.MovieActivity" />
        <activity android:name=".activity.base.BaseActivity" />
        <activity android:name=".activity.ExampleActivity" />
        <activity android:name=".activity.login.LoginActivity" />
        <activity android:name=".activity.rxjava.operators.DisposableExampleActivity"></activity>
        <activity android:name=".activity.rxjava.OperatorsActivity"></activity>
    </application>

</manifest>